#pragma once

namespace std
{

template<typename T, typename P> class pair
{
public:
    pair() :
        first(T()),
        second(P())
    {
    }

    pair(const T &first, const P &second) :
        first(first),
        second(second)
    {
    }

    pair(const pair &rhs) :
        first(rhs.first),
        second(rhs.second)
    {
    }

    pair<T, P> &operator=(const pair<T, P> &rhs)
    {
        first = rhs.first;
        second = rhs.second;

        return *this;
    }

    T first;
    P second;
};

} // namespace std
